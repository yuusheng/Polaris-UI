{
  "name": "polaris-ui",
  "version": "0.1.0",
  "private": false,
  "description": "基于Vue3 + TypeScript + TailwindCSS的前端组件库",
  "main": "lib/index.min.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "keyword": "vue3 ui",
  "license": "MIT",
  "author": "yuusheng <contact@yuusheng.cn>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:clean && npm run build:esm && npm run build:umd",
    "build:esm": "npx rollup -c ./build/rollup.esm.config.js",
    "build:umd": "npx rollup -c ./build/rollup.umd.config.js",
    "build:clean": "rimraf ./dist",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/server-renderer": "^3.2.31",
    "autoprefixer": "^9",
    "core-js": "^3.6.5",
    "eslint": "^8.10.0",
    "node-sass": "^7.0.1",
    "postcss": "^7",
    "postcss-import": "^14.0.2",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "vite": "^2.8.6",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-typescript": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/compiler-sfc": "^3.0.0",
    "rollup": "^2.58.0",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5",
    "vuepress": "^2.0.0-beta.36"
  },
  "repository": "git@github.com:Kevin-Lier/Polaris-UI.git"
}
